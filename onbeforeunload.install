 * Implementation of module installation hooks.
 */

/**
 * Implementation of hook_install().
 *
 * Ensure the javascript file of this module loaded first so that the
 * onBeforeUnload API is present when any other module loads.
 */
function onbeforeunload_install() {
  db_query("UPDATE {system} SET weight = -1000 WHERE name = 'onbeforeunload'");
}
